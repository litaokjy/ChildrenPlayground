<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="study_view">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>


    <declare-styleable name="CustomImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="round_radius" format="dimension" />
        <attr name="shape_type">
            <enum name="circle" value="0" />
            <enum name="round" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="customViewGroup">
        <attr name="vg_margin_top" format="dimension" />
        <attr name="vg_margin_left" format="dimension" />
        <attr name="vg_padding_top" format="dimension" />
        <attr name="vg_padding_left" format="dimension" />
    </declare-styleable>
</resources>
